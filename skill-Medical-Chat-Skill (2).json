    {
      "type": "standard",
      "title": "Unconscious",
      "output": {
        "generic": [
          {
            "title": "Unconscious",
            "options": [
              {
                "label": "< 1 year old (Unconscious, not-breathing)",
                "value": {
                  "input": {
                    "text": "Call 911 immediately. Place two fingers between the nipples. Push down with your fingers and do 30 compressions. After the 30 compressions, do two breaths, making sure there is nothing blocking the airway. If a breath comes back up, readjust the head and give another breath. If you can still feel the breath, continue doing compressions. If you don't feel the breath, do another one, then continue compressions. Repeat the compressions and breaths until an AED or EMS arrives. "
                  }
                }
              },
              {
                "label": "1-8 years old (Unconscious, not-breathing)",
                "value": {
                  "input": {
                    "text": "Call 911 immediately. Place one hand between the nipples, or two hands, one hand on top of the other, depending on the size of the child. Press down at least 2 inches and do 30 compressions. After this, make sure that there is nothing blocking the airway and do two breaths. If a breath comes back up, readjust the head and give another breath. If you can still feel the breath, continue doing compressions. If you don't feel the breath, do another one, then continue compressions. Repeat compressions and breaths until an AED or EMS arrives."
                  }
                }
              },
              {
                "label": "> 8 years old (Unconscious, not-breathing)",
                "value": {
                  "input": {
                    "text": "Call 911 immediately. Place one hand between the nipples and put the other hand on top of that hand. Press down at least 2 inches and do 30 compressions. After this, make sure that there is nothing blocking the airway and do two breaths. If a breath comes back up, readjust the head and give another breath. If you can still feel the breath, continue doing compressions. If you don't feel the breath, do another one, then continue compressions. Repeat compressions and breaths until an AED or EMS arrives."
                  }
                }
              },
              {
                "label": "Unconscious, Breathing",
                "value": {
                  "input": {
                    "text": "Call 911 immediately. Lay the victim down in semi prone position (raise one arm and roll the victim on that side and bend the opposite leg). Monitor vitals (airway, breathing, and circulation) and remember to keep track of breaths and beats per minute until EMS arrives. "
                  }
                }
              }
            ],
            "response_type": "option"
          },
